                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: {
                        position: 'bottom',
                    }
                }
            }
        });

        // Business Lines Performance Chart
        const businessLinesCtx = document.getElementById('businessLinesChart').getContext('2d');
        new Chart(businessLinesCtx, {
            type: 'bar',
