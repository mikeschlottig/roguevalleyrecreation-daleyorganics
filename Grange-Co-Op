                    borderWidth: 2,
                }
            },
            scales: {
                y: {
                    beginAtZero: true,
                    grid: {
                        color: 'rgba(86, 171, 47, 0.1)'
                    }
                },
                x: {
                    grid: {
                        color: 'rgba(86, 171, 47, 0.1)'
                    }
                }
            }
        };

        // Market Share Chart
        const marketShareCtx = document.getElementById('marketShareChart').getContext('2d');
        new Chart(marketShareCtx, {
            type: 'doughnut',
            data: {
                labels: ['Grange Co-op Market Share', 'Other Competitors'],
                datasets: [{
